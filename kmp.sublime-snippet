    <content><![CDATA[

int pf[N];
void pre(string s) {
    int n = (int) s.length();
    reset(pf,0);
    for (int i=1; i<n; ++i) {
        int j = pf[i-1];
        while (j > 0 && s[i] != s[j])
            j = pf[j-1];
        if (s[i] == s[j])  ++j;
        pf[i] = j;
    }
//    printA(pf,0,s.size());
}

]]></content>
